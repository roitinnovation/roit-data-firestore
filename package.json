    "@types/promise-timeout": "^1.3.3",
    "@types/shelljs": "^0.8.9",
    "@types/uuid": "^8.3.1",
    "copyfiles": "^2.4.1",
    "jest": "30.2.0",
    "redis": "4.0.6",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@google-cloud/firestore": "7.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@roit/roit-date": "1.10.5",
    "class-validator": "0.14.1",
    "class-validator-jsonschema": "5.1.0",
